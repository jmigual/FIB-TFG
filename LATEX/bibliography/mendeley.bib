number = {1},
pages = {1--8},
pmid = {28871110},
publisher = {Springer US},
title = {{A Deep Learning-Based Radiomics Model for Prediction of Survival in Glioblastoma Multiforme}},
url = {http://dx.doi.org/10.1038/s41598-017-10649-8},
volume = {7},
year = {2017}
}
